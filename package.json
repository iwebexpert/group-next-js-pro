{
  "name": "next-app",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "prisma": {
    "seed": "node --import=./register.js prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:prisma": "prisma format",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:reset": "dotenv -e .env.local -- prisma migrate reset",
    "prisma:deploy": "dotenv -e .env.local -- prisma migrate deploy && prisma generate"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.21.1",
    "bcryptjs": "^2.4.3",
    "dotenv-cli": "^7.4.2",
    "flowbite-react": "^0.10.2",
    "next": "14.2.16",
    "next-auth": "^4.24.10",
    "prisma": "^5.21.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.16",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
